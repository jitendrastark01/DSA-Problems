// JAVA CODE


class Solution {
    // Function to find the height of a binary tree.
    int height(Node node) {        
        // code here
        
        if(node==null){
            return -1;      
        }
        int leftheight=height(node.left);
        int rightheight=height(node.right);
        return Math.max(leftheight,rightheight)+1;
    }
}
