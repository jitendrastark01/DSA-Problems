// JAVA CODE


class Solution{
    Node reverseList(Node head) {
      
        Node prev=null;
        Node curr=head;
        Node next;
        
        while(curr!=null){
            next=curr.next;
            curr.next=prev;
            prev=curr;
            curr=next; 
        }
        
        head=prev;
        return head;
    }
}



#Time Complexity: O(n)
#Space Complexity: O(1)
